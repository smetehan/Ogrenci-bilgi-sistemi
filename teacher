public class Teacher {
    String name;
    String mpno;
    String branch;

    Teacher(String name, String mpno, String branch){
        this.name= name;
        this.mpno= mpno;
        this.branch= branch;


    }

    public void print() {
        System.out.println("adi :"+this.name);
        System.out.println("telefon :"+this.mpno);
        System.out.println("bolumu :"+this.branch);
    }
}
